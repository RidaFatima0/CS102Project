<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/loadingscre"
    android:layout_width="393dp"
    android:layout_height="800dp"
    android:clipToOutline="true"
    android:background="#FFFFFF"
    >

    <RelativeLayout android:id="@+id/navbar"
        android:layout_marginTop="0dp"
        android:layout_marginStart="0dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <View
            android:id="@+id/rectangle_1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/rectangle_1_shape" />

        <ImageView
            android:id="@+id/applogo"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="28dp"
            android:contentDescription="applogo"
            android:src="@drawable/applogo" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp"
            android:text="View Listing"
            android:textColor="#FFFFFF"
            android:textSize="30sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <RelativeLayout android:id="@+id/presentat_on_bubbles"
        android:layout_marginTop="338dp"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <View
            android:id="@+id/ellipse_1"
            android:background="@drawable/ellipse_1_shape"
            android:layout_marginStart="-1dp"
            android:layout_marginTop="-1dp"
            android:layout_width="13dp"
            android:layout_height="13dp" />

        <View
            android:id="@+id/ellipse_2"
            android:background="@drawable/ellipse_1_shape"
            android:layout_marginStart="29dp"
            android:layout_marginTop="-1dp"
            android:layout_width="13dp"
            android:layout_height="13dp" />

        <View
            android:id="@+id/ellipse_3"
            android:background="@drawable/ellipse_3_shape"
            android:layout_marginStart="59dp"
            android:layout_marginTop="-1dp"
            android:layout_width="13dp"
            android:layout_height="13dp" />

        <View   android:id="@+id/ellipse_4"
            android:background="@drawable/ellipse_4_shape"
            android:layout_marginStart="84dp"
            android:layout_marginTop="-1dp"
            android:layout_width="13dp"
            android:layout_height="13dp" />

    </RelativeLayout>

    <ImageView android:id="@+id/landImage"
        android:src="@drawable/image2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="129dp"
        android:layout_width="319dp"
        android:layout_height="211dp" />

    <ImageView android:id="@+id/image_3"
        android:src="@drawable/image_3"
        android:contentDescription="image_3"
        android:layout_marginStart="45dp"
        android:layout_marginTop="370dp"
        android:layout_width="70dp"
        android:layout_height="70dp" />

    <TextView android:id="@+id/mustafa_yildirim"
        android:textStyle="bold"
        android:layout_gravity="center_horizontal|center"
        android:gravity="center"
        android:textSize="14sp"
        android:textColor="@color/_bg__navbar_ek1_color"
        android:layout_marginLeft="118dp"
        android:layout_marginTop="379dp"
        android:layout_width="124.4dp"
        android:layout_height="35.4dp"
        android:text="@string/mustafa_yildirim_string"
        />

    <TextView android:id="@+id/_16_300_tl_year"
        android:textSize="14sp"
        android:textColor="@color/_16_300_tl_year_color"
        android:layout_marginLeft="125dp"
        android:layout_marginTop="410dp"
        android:layout_width="115.4dp"
        android:layout_height="30.4dp"
        android:text="@string/_16_300_tl_year_string"
        />


    <ImageView android:id="@+id/line_1"
        android:src="@drawable/line_1"
        android:contentDescription="line_1"
        android:layout_marginStart="13dp"
        android:layout_marginTop="465dp"
        android:layout_width="360dp"
        android:layout_height="1dp" />

    <TextView android:id="@+id/ankara"
        android:textSize="19sp"
        android:textColor="@color/_bg__navbar_ek1_color"
        android:layout_marginStart="42dp"
        android:layout_marginTop="475dp"
        android:layout_width="103.9dp"
        android:layout_height="25.9dp"
        android:text="Konya"
        />

    <ImageView android:id="@+id/location"
        android:src="@drawable/location"
        android:contentDescription="location"
        android:layout_marginStart="16dp"
        android:layout_marginTop="477dp"
        android:layout_width="22dp"
        android:layout_height="22dp" />

    <TextView android:id="@+id/land_usage_condition__agriculture"
        android:textSize="12sp"
        android:textColor="@color/_bg__navbar_ek1_color"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="501dp"
        android:layout_width="346.2dp"
        android:layout_height="24.2dp"
        android:text="@string/land_usage_condition__agriculture_string"
        />

    <ImageView android:id="@+id/line_4"
        android:src="@drawable/line_4"
        android:contentDescription="line_4"
        android:layout_marginStart="10dp"
        android:layout_marginTop="531dp"
        android:layout_width="360dp"
        android:layout_height="1dp" />

    <TextView
        android:id="@+id/descriptionTextList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="540dp"
        android:text="Description"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="565dp"
        android:text="Only our valued customer who will rent it will be able to understand how valuable this land actually is. However, since the agricultural infrastructure came to these lands, competition has become very intense here...See more.
"
        android:textColor="#605D5D"/>

    <ImageView android:id="@+id/line_5"
        android:src="@drawable/line_5"
        android:contentDescription="line_5"
        android:layout_marginStart="12dp"
        android:layout_marginTop="645dp"
        android:layout_width="360dp"
        android:layout_height="1dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="662dp"
        android:text="Rented By"
        android:textColor="#009688"
        android:textStyle="bold"/>


    <ImageView
        android:id="@+id/renterImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="690dp"
        app:srcCompat="@drawable/adam2" />

    <TextView android:id="@+id/renterLocation"
        android:textSize="14sp"
        android:textColor="@color/_16_300_tl_year_color"
        android:layout_marginLeft="125dp"
        android:layout_marginTop="720dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ankara"
        />

    <TextView android:id="@+id/renter"
        android:textStyle="bold"
        android:textSize="14sp"
        android:textColor="@color/_bg__navbar_ek1_color"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="695dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name: Fatih Cetin"
        />

    <ImageView android:id="@+id/renterloc"
        android:src="@drawable/location"
        android:layout_marginStart="100dp"
        android:layout_marginTop="720dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView android:id="@+id/line6"
        android:src="@drawable/line_5"
        android:layout_marginStart="12dp"
        android:layout_marginTop="765dp"
        android:layout_width="360dp"
        android:layout_height="1dp" />

</RelativeLayout>