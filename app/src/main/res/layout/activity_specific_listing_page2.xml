                        android:layout_width="174dp"
                        android:layout_height="38dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="5dp"
                        android:background="@drawable/rectangle_9_shape" />

                    <EditText
                        android:id="@+id/monthlyincometextbox"
                        android:layout_width="118.17dp"
                        android:layout_height="32.2dp"
                        android:layout_gravity="center_horizontal|center"
                        android:layout_marginLeft="29dp"
                        android:layout_marginTop="9dp"
                        android:gravity="center"
                        android:hint="@string/monthly_income_string"
                        android:textColor="@color/_bg__navbar_ek1_color"
                        android:textSize="12sp" />

                </RelativeLayout>
            </RelativeLayout>
        </ScrollView>

        <RelativeLayout
            android:layout_marginTop="-120dp"
            android:layout_marginStart="0dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
            <RelativeLayout android:id="@+id/login_button_ek4"
                android:layout_marginTop="730dp"
                android:layout_marginStart="67dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <View   android:id="@+id/rentbutton"
                    android:background="@drawable/rectangle_10_ek2_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="3dp"
                    android:layout_width="253dp"
                    android:layout_height="38dp" />

                <TextView android:id="@+id/rentTextView"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="16sp"
                    android:textColor="@color/_bg__specificlistingpage_ek2_color"
                    android:layout_marginLeft="17dp"
                    android:layout_marginTop="9dp"
                    android:layout_width="219dp"
                    android:layout_height="26.97dp"
                    android:text="@string/rent_string"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/elements"
                android:layout_width="wrap_content"
                android:layout_height="800dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="700dp">

                <View
                    android:id="@+id/requestsellercontactbutton"
                    android:layout_width="253dp"
                    android:layout_height="38dp"
                    android:layout_marginStart="68dp"
                    android:layout_marginTop="80dp"
                    android:background="@drawable/rectangle_10_shape" />

                <TextView
                    android:id="@+id/requestsellercontacttextview"
                    android:layout_width="218.4dp"
                    android:layout_height="25.37dp"
                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="85dp"
                    android:gravity="center"
                    android:text="@string/request_seller_contact_string"
                    android:textColor="@color/_bg__specificlistingpage_ek2_color"
                    android:textSize="14sp"
                    android:textStyle="bold" />

            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</HorizontalScrollView>